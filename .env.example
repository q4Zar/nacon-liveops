# Server Configuration
HTTP_SERVER_URL=http://go-nacon:8080
GRPC_SERVER_URL=go-nacon:8080
JWT_SECRET_KEY=your-secret-key

# Database
DB_PATH=/data/events.db

# Rate Limiter
RATE_LIMIT=100
RATE_LIMIT_WINDOW=60

# Circuit Breaker
CB_MAX_REQUESTS=100
CB_INTERVAL=60
CB_TIMEOUT=10
CB_CONSECUTIVE_FAILURES=5

# Stress Client
# Admin Credentials
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin-key-456
STRESS_DURATION=60s
GRPC_CONCURRENCY=10
HTTP_CONCURRENCY=10
REQUEST_DELAY=200ms
# Delete Operation Configuration
DELETE_TIMEOUT=10ms
DELETE_RETRIES=0

# Redis Configuration
REDIS_URL=redis:6379
REDIS_TTL=3600
REDIS_PASSWORD= 